
<!DOCTYPE html>
<html>
<head>
    <title>mLab MongoDB test</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
<div class="container">
    <h2>Database Results getting Routes </h2>
    <ul>
        <% results.forEach(function(r) { %>
        <li><%=r.CUSTOMER_ID%></li>
        <li><%=r.BILLING_ID%></li>
        <li><%=r.SHIPPING_ID%></li>
        <li><%=r.DATE%></li>
        <li><%=r.PRODUCT_VECTOR%></li>
        <li><%=r.ORDER_TOTAL%></li>
        <br/> <br/>
        <% }); %>
    </ul>
</div>

</body>
</html>
